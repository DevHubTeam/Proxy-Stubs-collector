    "babel-register": "^6.11.6",
    "mocha": "^3.0.1",
    "nodemon": "^1.10.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "decompress-response": "^3.3.0",
    "express": "^4.16.3",
    "express-session": "^1.15.6",
    "node-storage": "^0.0.7",
    "promise.pipe": "^3.0.0",
    "request": "^2.87.0",
